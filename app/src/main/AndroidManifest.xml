    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_bus_dayeon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_bus_dayeon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Step2BusInfoActivity"></activity>
        <activity android:name=".IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Step2BusArriveActivity" />

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-6036255824922099~1780975018" />

        <activity android:name=".FavoriteActivity" />
        <activity android:name=".BusArriveActivity" />
        <activity android:name=".BusInfoActivity" />
        <activity android:name=".MainActivity"></activity>
    </application>

</manifest>